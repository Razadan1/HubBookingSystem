<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HubBookingSystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HubBookingSystem.styles.css" asp-append-version="true" />
    <style></style>
</head>
<body>
    <header>
      <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-primary border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">HubBookingSystem</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="AboutUs">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ContactUs">Contact Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
         </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            <partial name="_Notification" />
            @RenderBody()
        </main>
    </div>
    <div class="container my-5 bg-dark">
        <footer class="bg-dark text-center text-lg-start text-white">
            <div class="container p-4">
                <div class="row my-4">
                    <div class="col-lg-3 col-md-6 mb-4 mb-md-0">
                        <h5 class="text-uppercase mb-4">Address</h5>
                        <ul class="list-unstyled mb-0">
                            <li>
                                MKAN Secretariat
                            </li>
                            <li>
                                Ahmadiyya, Ojokoro
                            </li>
                            <li>
                                Lagos, Nigeria
                            </li>
                        </ul>
                    </div>
                   
                    <div class="col-lg-3 col-md-6 mb-4 mb-md-0">
                        <h5 class="text-uppercase">Links</h5>
                        <ul class="list-unstyled mb-0">
                            <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Index">Home</a></li>
                            <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="AboutUs">About us</a></li>
                            <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Services">Services</a></li>
                            <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Privacy">Privacy</a></li>
                        </ul>
                    </div>

                    <div class="col-lg-3 col-md-6 mb-4 mb-md-0">
                        <h5 class="text-uppercase mb-0">Our Social Networks</h5>
                        <p>You can reach out to our social handles</p>
                        <ul class="list-unstyled d-flex flex-row justify-content-center">
                            <li>
                                <a href="https://twitter.com/Razadan1" class="twitter"><i class="bx bxl-twitter"></i></a>
                            </li>
                            <li>
                                <a href="https://web.facebook.com/danmole.omomayowa?_rdc=2&_rdr"><i class="fab fa-facebook-square"></i></a>
                            </li>
                            <li>
                                <a href="https://www.instagram.com/razadan1/"><i class="bx bxl-instagram"></i></a>
                            </li>
                            <li>
                                <a href="https://www.linkedin.com/in/ahmadraza-danmole-62202122a/"><i class="bx bxl-linkedin"></i></a>
                            </li>
                        </ul>
                     </div>
                       
                    <div class="col-lg-3 col-md-6 mb-4 mb-md-0">
                        <h5 class="text-uppercase mb-0">Contact</h5>
                        <ul class="list-unstyled">
                            <li>
                                <p><i class="fas fa-map-marker-alt pe-2"></i>Ojokoro, No. 17 Bola</p>
                            </li>
                            <li>
                                <p><i class="fas fa-phone pe-2"></i>+234 8027027725</p>
                            </li>
                            <li>
                                <p><i class="fas fa-envelope pe-2 mb-0"></i> <a href="mailto:danmoleomomayowa@gmail.com">danmoleomomayowa@gmail.com</a></p>
                            </li>
                        </ul>
                     </div>
                </div>
            </div>
            <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2)">
                &copy; 2024 - HubBookingSystem - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                Make Your  bookings <i class="bi bi-laptop"></i> Book a space
            </div>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
